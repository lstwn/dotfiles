{
  "coc.preferences.useQuickfixForLocations": true,
  "suggest.languageSourcePriority": 200,
  "diagnostic.enableMessage": "jump",
  "diagnostic.infoSign": "i",
  "diagnostic.hintSign": "i",
  "diagnostic.warningSign": ">",
  "diagnostic.errorSign": "x",
  "eslint.autoFixOnSave": true,
  "coc.preferences.formatOnSaveFiletypes": ["javascript", "typescript", "graphql"],
  "latex.lint.onSave": true,
  "latex.lint.onChange": false,
  "latex.build.onSave": true,
  "latex.build.executable": "latexmk",
  "latex.build.args": ["-lualatex", "-outdir=build"],
  "latex.build.outputDirectory": "build",
  "languageserver": {
    "graphql": {
      "command": "graphql-lsp",
      "args": ["server", "-m", "stream"],
      "filetypes": ["typescript", "graphql"]
    }
  }
}
