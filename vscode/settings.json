  "terminal.integrated.drawBoldTextInBrightColors": false,
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.cursorStyle": "block",
  "terminal.integrated.cursorBlinking": false,
  "terminal.integrated.fontSize": 14,
  "breadcrumbs.enabled": true,
  "editor.fontFamily": "'DejaVu Sans Mono', 'monospace'",
  "editor.fontSize": 14,
  "editor.formatOnType": true,
  "editor.cursorSurroundingLines": 10,
  "editor.suggestSelection": "first",
  "editor.renderWhitespace": "all",
  "editor.wordWrap": "on",
  "editor.cursorBlinking": "solid",
  "editor.foldingHighlight": true,
  "editor.rulers": [80],
  "editor.tabSize": 4,
  "editor.acceptSuggestionOnEnter": "off",
  "editor.quickSuggestions": {
    "other": true,
    "comments": false,
    "strings": true
  },
  "editor.semanticHighlighting.enabled": "configuredByTheme",
  "zenMode.hideLineNumbers": false,
  "zenMode.hideStatusBar": false,
  "zenMode.hideTabs": false,
  "git.enableSmartCommit": true,
  "git.autofetch": true,
  "markdown.preview.fontSize": 15,
  "markdown.preview.fontFamily": "'DejaVu Sans', -apple-system, BlinkMacSystemFont, 'Segoe WPC', 'Segoe UI', 'Ubuntu', 'Droid Sans', sans-serif",
  "markdown.preview.scrollEditorWithPreview": false,
  "telemetry.enableTelemetry": false,
  "vim.showmodename": true,
  "vim.debug.silent": true,
  "vim.useSystemClipboard": true,
  "vim.easymotion": true,
  "vim.incsearch": true,
  "vim.highlightedyank.enable": true,
  "vim.highlightedyank.color": "rgba(120, 120, 120, 0.5)",
  "vim.highlightedyank.duration": 500,
  "vim.useCtrlKeys": true,
  "vim.leader": ",",
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["g", "d"],
      "commands": ["editor.action.peekDefinition"]
    },
    {
      "before": ["g", "y"],
      "commands": ["editor.action.goToTypeDefinition"]
    },
    {
      "before": ["g", "r"],
      "commands": ["editor.action.referenceSearch.trigger"]
    },
    {
      "before": ["]", "g"],
      "commands": ["editor.action.marker.nextInFiles"]
    },
    {
      "before": ["[", "g"],
      "commands": ["editor.action.marker.prevInFiles"]
    },
    {
      "before": ["<leader>", "r", "n"],
      "commands": ["editor.action.rename"]
    },
    {
      "before": ["<leader>", "f"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["<leader>", "e"],
      "commands": ["workbench.view.explorer"]
    },
    {
      "before": ["<leader>", "g"],
      "commands": ["workbench.view.scm"]
    },
    {
      "before": ["<leader>", "r"],
      "commands": ["workbench.action.replaceInFiles"]
    },
    {
      "before": ["<leader>", "c"],
      "commands": ["workbench.action.terminal.focus"]
    },
    {
      "before": ["<leader>", "d"],
      "commands": ["workbench.actions.view.problems"]
    },
    {
      "before": ["<leader>", "o"],
      "commands": ["workbench.action.gotoSymbol"]
    },
    {
      "before": ["<leader>", "s"],
      "commands": ["workbench.action.showAllSymbols"]
    },
    {
      "before": [":"],
      "commands": ["workbench.action.showCommands"]
    },
  ],
  "files.associations": {
    "*.txt": "markdown",
    "*.csv": "CSV (pipe)",
    "*.prisma": "graphql"
  },
  "[markdown]": {
    "editor.wordWrap": "on",
    "editor.quickSuggestions": true,
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  },
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "latex-workshop.view.pdf.viewer": "tab",
  "latex-workshop.latex.outDir": "build",
  "latex-workshop.latex.recipes": [
    {
      "name": "lualatex ➞ biber ➞ lualatex",
      "tools": ["lualatex", "biber", "lualatex"]
    },
    {
      "name": "latexmk (with lualatex)",
      "tools": ["lualatexmk"]
    }
  ],
  "latex-workshop.latex.tools": [
    {
      "name": "lualatex",
      "command": "lualatex",
      "args": [
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "-output-format=pdf",
        "-output-directory=%OUTDIR%",
        "%DOC%"
      ],
      "env": {}
    },
    {
      "name": "biber",
      "command": "biber",
      "args": ["%OUTDIR%/%DOCFILE%"],
      "env": {}
    },
    {
      "name": "lualatexmk",
      "command": "latexmk",
      "args": [
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "-lualatex",
        "-outdir=%OUTDIR%",
        "%DOC%"
      ],
      "env": {}
    }
  ],
  "terminal.integrated.shell.linux": "/usr/bin/tmux",
  "todo-tree.highlights.enabled": true,
  "todo-tree.tree.showScanModeButton": false,
  "[vue]": {
    "editor.defaultFormatter": "octref.vetur"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "yaml.schemas": {
    "file:///home/lstwn/.vscode/extensions/atlassian.atlascode-2.6.6/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
  },
  "Notes.notesLocation": "/home/lstwn/Documents/Notes",
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.rulers": [80],
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": false
  },
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "cSpell.enabled": false,
  "workbench.colorTheme": "GitHub Dark",
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "rust-analyzer.inlayHints.enable": false,
  "workbench.activityBar.visible": false,
  "terminal.integrated.commandsToSkipShell": [
    "-workbench.action.quickOpen"
  ],
}
